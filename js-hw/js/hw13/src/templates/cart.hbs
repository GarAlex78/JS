{{#each arrayFoto}}
<div class="grid-item">
    <img class="photo-card__img" src="{{this.webformatURL}}" alt="" data-url="{{this.largeImageURL}}" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons md">thumb_up</i>
            {{this.likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons md">visibility</i>
            {{this.views}}
        </p>
        <p class="stats-item">
            <i class="material-icons md">comment</i>
            {{this.comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons md">cloud_download</i>
            {{this.downloads}}
        </p>
    </div>
</div>
{{/each}}